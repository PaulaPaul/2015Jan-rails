<!DOCTYPE html>
<html>
<head>
  <title>TheForce</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
    <!-- Materialize Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.95.3/css/materialize.min.css">
  <!-- Materialize Compiled and minified JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.95.3/js/materialize.min.js"></script>
  <script>
    $( document ).ready(function(){
       $(".button-collapse").sideNav();
    });
  </script>
  
  </head>
<body>
   <nav>
    <div class="nav-wrapper">
      <a href="#!" class="brand-logo">The Force</a>
      <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="mdi-navigation-menu"></i></a>
      <ul class="right hide-on-med-and-down">
        <li><%= link_to "Films", pages_film_path %><i class="mdi-action-android"></i></li>
        <li><%= link_to "People", pages_people_path %><i class="mdi-action-android"></i></li>
        <li><%= link_to "Planets", pages_planets_path %><i class="mdi-action-android"></i></li>
        <li><%= link_to "Species", pages_species_path %><i class="mdi-action-android"></i></li>
        <li><%= link_to "Starships", pages_starships_path %><i class="mdi-action-android"></i></li>
        <li><%= link_to "Vehicles", pages_vehicles_path %><i class="mdi-action-android"></i></li>
      </ul>
      <ul class="side-nav" id="mobile-demo">
        <li><%= link_to "Films", pages_film_path %></li>
        <li><%= link_to "People", pages_people_path %></li>
        <li><%= link_to "Planets", pages_planets_path %></li>
        <li><%= link_to "Species", pages_species_path %></li>
        <li><%= link_to "Starships", pages_starships_path %></li>
        <li><%= link_to "Vehicles", pages_vehicles_path %></li>
      </ul>
    </div>
  </nav>
  

<%= yield %>

</body>
</html>
